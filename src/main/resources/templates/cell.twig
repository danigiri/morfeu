{	"schema": 0
	,"URI": "$[cell.URI]$"
	,"name": "$[cell.Name]$"
	,"desc": "$[cell.Desc | trim]$"
	,"value": "$[cell.Value]$"
	,"cellModelURI": "$[cell.CellModel.URI]$"
	,"isSimple": $[cell.isSimple]$
	$( if cell.isComplex )$
		,"attributes": [$( for a in cell.asComplex.attributes.asList )$
				$( include "cell.twig" with {'cell': a} only )$ $( if (loop.last) )$ $(else)$,$(endif)$
			$( endfor )$]
		,"children": [$( for c in cell.asComplex.children.asList )$
				$( include "cell.twig" with {'cell': c} only )$ $( if (loop.last) )$ $(else)$,$(endif)$
			$( endfor )$]
	$( endif )$
	
}