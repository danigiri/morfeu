{# this template takes all the attributes of a complex cell and prints them out as yaml, taking care of -#}
{# the fact that if we have an identifier we put it first and use the compact mapping form -#}
{# hack to put the identifier first -#}
{%- if cell.cellModel.metadata.identifier.isPresent -%}
	{%- set id = cell.cellModel.metadata.identifier.get -%}
	{%- set indentid = slice(indent, 2, length(indent)) -%}
	{%- set a = cell.asComplex.attributes.attribute(id) -%}
	{{-indentid}}- {{a.name}}: {% if a.value.isPresent %}{{a.value.get}}{% endif %}
{% else -%}
	{%- set id = '' -%}
{%- endif -%}
{%- for a in cell.asComplex.attributes.asList -%}
{%- if a.name!=id -%}
	{{-indent}}{{a.name}}: {% if a.value.isPresent %}{{a.value.get}}{% endif %}
{% endif -%}
{% endfor -%}