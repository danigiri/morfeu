{
	"schema" : 0
	,"name": "$[v.Name]$"
	,"desc": "$[v.Desc]$"
	,"valid": true
	$( if defined(problem) )$
		,"problem": "$[ problem ]$"
	$( endif )$
	
	,"cellmodels": [$( for cellModel in v.rootCellModels )$
			$( include "cell-model.twig" with {'cm': cellModel} only )$ $( if (loop.last) )$ $(else)$,,$(endif)$
		$( endfor )$]
}